<%
function PartsToKorean(strPart)
	dim strKorean
	Select Case strPart
		Case "JaJe"
			strKorean = "자재"
		Case "IMT"
			strKorean = "IMT"
		Case "SMT"
			strKorean = "SMT"
		Case "JeJo2"
			strKorean = "제조2"
		Case "JeJo3"
			strKorean = "제조3"
		Case "Eng"
			strKorean = "기술"
		Case "IQC"
			strKorean = "수입검사"
		Case "PCBA_QC"
			strKorean = "PCBA 검사"
		Case "CBOX_QC"
			strKorean = "CBOX 검사"
		Case "SPMK"
			strKorean = "슈퍼마켓"
		Case "DLV"
			strKorean = "영업"
		Case "Price"
			strKorean = "단가"
		Case "OTP"
			strKorean = "OTP"
		Case "SMTech"
			strKorean = "에스엠텍"
		Case "DSTech"
			strKorean = "DSTech"
	End Select
	PartsToKorean = strKorean
end function 
